{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "allowJs": false,
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "dom"
        ],
        "baseUrl": ".",
        "paths": {
            "config/appSettings": [
                "src/config/appSettings.dev.ts",
            ]
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}